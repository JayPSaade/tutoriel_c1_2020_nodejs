{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "trailing-comma": false,
        "quotemark": false,
        "arrow-parens": false,
        "semicolon": [true, "always", "ignore-bound-class-methods"],
        "object-literal-sort-keys": false,
        "max-classes-per-file": false,
        "variable-name": false
    },
    "rulesDirectory": []
}
